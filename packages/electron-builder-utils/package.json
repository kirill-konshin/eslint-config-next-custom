{
  "name": "@kirill.konshin/electron-builder-utils",
  "version": "0.0.1",
  "type": "module",
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": "./build/index.js",
    "./images": "./build/images.js",
    "./builder": "./build/builder.js"
  },
  "scripts": {
    "clean": "rm -rf .tscache tsconfig.tsbuildinfo build",
    "build": "tsc --build",
    "build:index": "cti create ./src",
    "start": "yarn build --watch --preserveWatchOutput",
    "wait": "wait-on build/index.js"
  },
  "dependencies": {
    "psd": "^3.4.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/psd": "^3.4.3",
    "create-ts-index": "^1.14.0",
    "electron": "^34.0.1",
    "electron-builder": "^25.1.8",
    "typescript": "^5.7.3",
    "wait-on": "^8.0.2"
  },
  "peerDependencies": {
    "electron": "^34.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Kirill Konshin <kirill@konshin.org> (https://konshin.org)",
  "license": "MIT",
  "description": ""
}
