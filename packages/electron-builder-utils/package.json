{
  "name": "@kirill.konshin/electron-builder-utils",
  "description": "",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf .tscache dist",
    "build": "tsup",
    "build:index": "cti create ./src",
    "start": "yarn build --watch",
    "wait": "wait-on ./dist/index.js"
  },
  "dependencies": {
    "psd": "^3.4.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/psd": "^3.4.3",
    "electron": "^34.0.1",
    "electron-builder": "^25.1.8"
  },
  "peerDependencies": {
    "electron": "^34.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Kirill Konshin <kirill@konshin.org> (https://konshin.org)",
  "license": "MIT",
  "exports": {
    "./builder": {
      "types": "./dist/builder.d.ts",
      "require": "./dist/builder.cjs",
      "import": "./dist/builder.mjs"
    },
    "./images": {
      "types": "./dist/images.d.ts",
      "require": "./dist/images.cjs",
      "import": "./dist/images.mjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  }
}