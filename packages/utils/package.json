      "types": "./dist/cache.d.ts",
      "require": "./dist/cache.cjs",
      "import": "./dist/cache.mjs"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "require": "./dist/errors.cjs",
      "import": "./dist/errors.mjs"
    },
    "./measure": {
      "types": "./dist/measure.d.ts",
      "require": "./dist/measure.cjs",
      "import": "./dist/measure.mjs"
    },
    "./mutex": {
      "types": "./dist/mutex.d.ts",
      "require": "./dist/mutex.cjs",
      "import": "./dist/mutex.mjs"
    },
    "./worker": {
