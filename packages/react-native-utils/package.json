{
  "name": "@kirill.konshin/react-native-utils",
  "description": "",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf .tscache dist",
    "build": "tsup",
    "build:index": "cti create ./src",
    "start": "yarn build --watch",
    "wait": "wait-on ./dist/index.js"
  },
  "dependencies": {
    "expo-file-system": "^18.0.7",
    "expo-sharing": "^13.0.1",
    "expo-updates": "^0.26.13"
  },
  "devDependencies": {
    "react-native": "^0.77.0"
  },
  "peerDependencies": {
    "react": "^18",
    "react-native": "^0.77.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Kirill Konshin <kirill@konshin.org> (https://konshin.org)",
  "license": "MIT",
  "exports": {
    "./share": {
      "types": "./dist/share.d.ts",
      "require": "./dist/share.cjs",
      "import": "./dist/share.mjs"
    },
    "./update": {
      "types": "./dist/update.d.ts",
      "require": "./dist/update.cjs",
      "import": "./dist/update.mjs"
    },
    "./useAppState": {
      "types": "./dist/useAppState.d.ts",
      "require": "./dist/useAppState.cjs",
      "import": "./dist/useAppState.mjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  }
}