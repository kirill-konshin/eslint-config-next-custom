      "require": "./dist/genericControl.cjs",
      "import": "./dist/genericControl.mjs"
    },
    "./readOnly": {
      "types": "./dist/readOnly.d.ts",
      "require": "./dist/readOnly.cjs",
      "import": "./dist/readOnly.mjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
