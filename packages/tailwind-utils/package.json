{
  "name": "@kirill.konshin/tailwind-utils",
  "description": "",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf .tscache dist",
    "build": "tsup",
    "build:index": "cti create ./src",
    "start": "yarn build --watch",
    "wait": "wait-on ./dist/index.js"
  },
  "devDependencies": {
    "tailwindcss": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^18",
    "tailwindcss": "^4"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Kirill Konshin <kirill@konshin.org> (https://konshin.org)",
  "license": "MIT",
  "exports": {
    "./fullpage": {
      "types": "./dist/fullpage.d.ts",
      "require": "./dist/fullpage.cjs",
      "import": "./dist/fullpage.mjs"
    },
    "./responsiveHelper": {
      "types": "./dist/responsiveHelper.d.ts",
      "require": "./dist/responsiveHelper.cjs",
      "import": "./dist/responsiveHelper.mjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  }
}