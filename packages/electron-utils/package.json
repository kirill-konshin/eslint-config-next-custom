    "wait": "wait-on ./dist/index.js"
  },
  "dependencies": {
    "electron-default-menu": "^1.0.2",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.9"
  },
  "devDependencies": {
    "@types/psd": "^3.4.3",
    "electron": "^34.0.1"
  },
  "peerDependencies": {
    "electron": "^34.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Kirill Konshin <kirill@konshin.org> (https://konshin.org)",
  "license": "MIT",
  "exports": {
    "./createWindow": {
      "types": "./dist/createWindow.d.ts",
      "require": "./dist/createWindow.cjs",
      "import": "./dist/createWindow.mjs"
    },
    "./updater": {
      "types": "./dist/updater.d.ts",
      "require": "./dist/updater.cjs",
      "import": "./dist/updater.mjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  }
