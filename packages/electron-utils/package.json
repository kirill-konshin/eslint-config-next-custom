{
  "name": "@kirill.konshin/electron-utils",
  "description": "",
  "version": "0.0.5",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf .tscache dist",
    "build": "tsup",
    "build:index": "cti create ./src",
