{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "noEmit": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "useDefineForClassFields": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": ["node_modules"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"]
}
